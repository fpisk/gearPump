FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


#include "${FOAM_CASE}/constant/caseSettings"

pOut 0.2;

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0.0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

   inlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    outlet
    {
        type            uniformTotalPressure;      
        value           $pOut;
        psi             psi;
        rho             rho;
        gamma           1.4; 
    }
    
    walls
    {
        type            zeroGradient;
    }
 
    frontAndBack
    {
        type            empty;
    }

    "nonCouple.*"
    {
        type            zeroGradient;
    }
}


